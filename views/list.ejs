
    <%- include("header") -%>  
    
    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>

    <div class="box">
        <%for(let i = 0; i < listElement.length ; i++){ %>
            <div class="item">
                <input type="checkbox">
                <p><%=listElement[i]%></p>
            </div>
            
        <%} %>   
        <form action="/" method="post">
        <input type="text" name="listElement" placeholder="New Item">
        <button type="submit" name="Button" value=<%= listTitle %>>+</button>
        </form>   
    </div>
   

    <%- include("footer") -%>  

    
